<script src="/assets/semantic/js/html2canvas.min.js"></script>

<script > 
    $( "#printer" ).click(function() {
        html2canvas(document.getElementById('exportthis'), {
            onrendered: function (canvas) {
                var data = canvas.toDataURL();
                var docDefinition = {
                    content: [{
                        image: data,
                        width: 500,
                    }] 
                };
                pdfMake.createPdf(docDefinition).download("Scres.pdf");
            }
        });

    })
   
